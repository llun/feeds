{
  "name": "feeds-fetcher",
  "version": "2.5.4",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/feeds",
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "load": "INPUT_OPMLFILE='feeds.opml' node -r @swc-node/register index.ts",
    "loadFile": "INPUT_OPMLFILE='feeds.opml' INPUT_STORAGETYPE='files' node -r @swc-node/register index.ts",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx",
      "js"
    ],
    "failWithoutAssertions": false,
    "require": [
      "@swc-node/register"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^19.0.13",
    "@tailwindcss/typography": "^0.5.9",
    "@vscode/sqlite3": "^5.1.2",
    "autoprefixer": "^10.4.15",
    "ava": "^5.3.1",
    "date-fns": "^2.30.0",
    "history": "^5.3.0",
    "html-minifier": "^4.0.0",
    "html-react-parser": "^4.2.2",
    "jsdom": "^22.1.0",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "next": "^13.4.19",
    "node-fetch": "2",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.11.0",
    "sql.js-httpvfs": "^0.8.12",
    "sqlite3": "^5.1.6",
