    "@swc-node/core": "^1.10.6",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.93",
    "@types/html-minifier": "^4.0.4",
    "@types/jsdom": "^21.1.4",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@types/sanitize-html": "^2.9.3",
    "@types/sinon": "^10.0.20",
    "@types/xml2js": "^0.4.13",
    "sinon": "^16.1.3",
    "typescript": "^5.2.2"
  }
