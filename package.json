{
  "name": "feeds-fetcher",
  "version": "4.0.0",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "type": "module",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/feeds",
  "license": "ISC",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "load": "INPUT_OPMLFILE='feeds.opml' node --import tsx index.ts",
    "loadFile": "INPUT_OPMLFILE='feeds.opml' INPUT_STORAGETYPE='files' node --import tsx index.ts",
    "test": "NODE_OPTIONS='--import tsx' ava"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx",
      "js"
    ],
    "failWithoutAssertions": false
  },
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@octokit/rest": "^22.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@vscode/sqlite3": "^5.1.12-vscode",
    "autoprefixer": "^10.4.24",
    "date-fns": "^4.1.0",
    "history": "^5.3.0",
    "html-minifier": "^4.0.0",
    "html-react-parser": "^5.2.17",
    "knex": "^3.1.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sanitize-html": "^2.17.0",
    "shadcn": "^3.8.4",
    "sql.js-httpvfs": "^0.8.12",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.18",
    "tailwindcss-cli": "^0.1.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.6",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "@types/sinon": "^21.0.0",
    "@types/xml2js": "^0.4.14",
    "ava": "^6.4.1",
    "prettier": "^3.8.1",
    "sinon": "^21.0.1",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@octokit/rest": "22.0.1"
  },
  "packageManager": "yarn@4.12.0"
}
