    "@swc-node/core": "^1.13.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.9.2",
    "@types/html-minifier": "^4.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/sanitize-html": "^2.13.0",
    "@types/sinon": "^17.0.3",
    "@types/xml2js": "^0.4.14",
    "sinon": "^19.0.2",
    "typescript": "^5.6.3"
  }
