{
  "name": "feeds-fetcher",
  "version": "2.6.0",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/feeds",
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "load": "INPUT_OPMLFILE='feeds.opml' node -r @swc-node/register index.ts",
    "loadFile": "INPUT_OPMLFILE='feeds.opml' INPUT_STORAGETYPE='files' node -r @swc-node/register index.ts",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx",
      "js"
    ],
    "failWithoutAssertions": false,
    "require": [
      "@swc-node/register"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@vscode/sqlite3": "^5.1.2",
    "autoprefixer": "^10.4.16",
    "ava": "^5.3.1",
    "date-fns": "^2.30.0",
    "history": "^5.3.0",
    "html-minifier": "^4.0.0",
    "html-react-parser": "^4.2.6",
    "jsdom": "^22.1.0",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "node-fetch": "3",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.11.0",
    "sql.js-httpvfs": "^0.8.12",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.3.3",
