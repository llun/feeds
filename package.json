    "@swc-node/core": "^1.10.2",
    "@swc-node/register": "^1.6.7",
    "@swc/core": "^1.3.83",
    "@types/html-minifier": "^4.0.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/sanitize-html": "^2.9.0",
    "@types/sinon": "^10.0.16",
    "@types/xml2js": "^0.4.12",
    "sinon": "^15.2.0",
    "typescript": "^5.2.2"
  }
