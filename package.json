{
  "name": "feeds-fetcher",
  "version": "3.0.4",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/feeds",
  "license": "ISC",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "load": "INPUT_OPMLFILE='feeds.opml' node -r @swc-node/register index.ts",
    "loadFile": "INPUT_OPMLFILE='feeds.opml' INPUT_STORAGETYPE='files' node -r @swc-node/register index.ts",
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx",
      "js"
    ],
    "failWithoutAssertions": false,
    "require": [
      "@swc-node/register"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/rest": "^22",
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.12.7",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vscode/sqlite3": "^5.1.2",
    "autoprefixer": "^10.4.21",
    "date-fns": "^4.1.0",
    "history": "^5.3.0",
    "html-minifier": "^4.0.0",
    "html-react-parser": "^5.2.5",
    "jsdom": "^26.1.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.522.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "node-fetch": "3",
    "postcss": "^8.5.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanitize-html": "^2.17.0",
    "shadcn": "2.7.0",
    "sql.js-httpvfs": "^0.8.12",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.10",
    "tailwindcss-cli": "^0.1.2",
    "tw-animate-css": "^1.3.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.5",
    "@types/jsdom": "^21.1.7",
