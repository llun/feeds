    "@swc-node/core": "^1.10.6",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.100",
    "@types/html-minifier": "^4.0.5",
    "@types/jsdom": "^21.1.5",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/sanitize-html": "^2.9.5",
    "@types/sinon": "^17.0.2",
    "@types/xml2js": "^0.4.14",
    "sinon": "^17.0.1",
    "typescript": "^5.3.2"
  }
